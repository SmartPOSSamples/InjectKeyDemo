<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cloudpos.injectkey.demo">
    <!-- Access internet permission -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Access inject key server permission -->
    <uses-permission android:name="android.permission.CLOUDPOS_REMOTE_KEY_INJECTION" />
    <!-- Read safe module permission -->
    <uses-permission android:name="android.permission.CLOUDPOS_SAFE_MODULE_READONLY"/>
    <!-- write safe module permission -->
    <uses-permission android:name="android.permission.CLOUDPOS_SAFE_MODULE"/>
    <!-- Update user key of pin permission -->
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_UPDATE_USER_KEY"/>
    <!-- Update master key of pin permission -->
    <uses-permission android:name="android.permission.CLOUDPOS_PIN_UPDATE_MASTER_KEY"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.cloudpos.injectkey.demo.MainActivity"
            android:enabled="true"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>